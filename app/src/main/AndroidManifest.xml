<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yosola.android.mexicanquiz">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.yosola.android.mexicanquiz.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.yosola.android.mexicanquiz.MultipleAnswersActivity"
            android:label="@string/quiz"/>
        <activity android:name="com.yosola.android.mexicanquiz.WrittenAnswerActivity"
            android:label="@string/quiz"/>
        <activity android:name="com.yosola.android.mexicanquiz.TrueFalseActivity"
            android:label="@string/quiz"/>
        <activity android:name="com.yosola.android.mexicanquiz.RadioAnswerActivity"
            android:label="@string/quiz"/>
        <activity android:name="com.yosola.android.mexicanquiz.FinalActivity"
            android:label="@string/quiz"/>

    </application>

</manifest>